{
    "name": "eslint-config-facile",
    "version": "0.9.1",
    "description": "facile.it ESLint extensible configuration",
    "main": "index.js",
    "repository": "https://github.com/facile-it/eslint-config-facile",
    "author": "Iacopo <siuvdlec@gmail.com>",
    "license": "MIT",
    "files": [
        "index.js",
        "react.js",
        "node.js",
        "next.js",
        "prettierrc.json"
    ],
    "scripts": {
        "prebuild": "rm -f ./index.js ./react.js ./node.js ./prettierrc.json",
        "build": "tsc && cp .prettierrc.json prettierrc.json",
        "pretest": "npm run build",
        "test": "eslint ./test/. -c ./test/_config.js --ext .ts",
        "prepublish": "npm run build"
    },
    "peerDependencies": {
        "eslint": "^7.0.0 || ^8.0.0",
        "prettier": "^2.5",
        "typescript": "^4.0 || ^5.0"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "eslint-config-next": "^13.3.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-fp-ts": "^0.3.2",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-rxjs": "^5.0.3"
    },
    "devDependencies": {
        "@types/node": "^18.6.4",
        "eslint": "^8.31.0",
        "prettier": "^2.8.1",
        "typescript": "^4.9.4"
    },
    "tags": [
        "eslint",
        "prettier",
        "configuration"
    ],
    "keywords": [
        "typescript",
        "react",
        "eslint",
        "prettier",
        "extensible",
        "configuration"
    ]
}
